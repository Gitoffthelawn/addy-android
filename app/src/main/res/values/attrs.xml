<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SectionView">
        <attr name="sectionTitle" format="string" />
        <attr name="sectionDescription" format="string" />
        <attr name="sectionStartIcon" format="integer" />
        <attr name="sectionEndIcon" format="integer" />
        <attr name="sectionShowSwitch" format="boolean" />
        <attr name="sectionSwitchChecked" format="boolean" />
        <attr name="sectionRippleEffect" format="boolean" />
        <attr name="sectionEnabled" format="boolean" />
        <attr name="sectionColorAccent" format="color" />
        <attr name="sectionAlert" format="boolean" />
        <attr name="sectionOutlineWidth" format="integer" />
        <attr name="sectionElevation" format="float" />
    </declare-styleable>

    <declare-styleable name="AppWidgetAttrs">
        <attr name="appWidgetPadding" format="dimension" />
        <attr name="appWidgetInnerRadius" format="dimension" />
        <attr name="appWidgetRadius" format="dimension" />
    </declare-styleable>
</resources>